{
  "name": "@rachel-barrett/variant-ts",
  "version": "2.0.0",
  "description": "A library for variant types in typescript",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": "./lib/index.js",
    "./churchVariant": "./lib/churchVariant.js"
  },
  "typesVersions": {
    "*": {
      "lib/index.d.ts": [
        "lib/index.d.ts"
      ],
      "*": [
        "lib/*"
      ]
    }
  },
  "scripts": {
    "build": "tsc --build",
    "format": "prettier --write .",
    "test": "cd test; ts-node index.ts",
    "clean": "rm -r lib"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rachel-barrett/variant-ts.git"
  },
  "keywords": [
    "algebraic-data-types",
    "functional-programming",
    "typescript",
    "fp-ts",
    "variant"
  ],
  "author": "Rachel Barrett",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rachel-barrett/variant-ts/issues"
  },
  "homepage": "https://github.com/rachel-barrett/variant-ts#readme",
  "devDependencies": {
    "@rachel-barrett/test-js": "^2.0.0",
    "fp-ts": "^2.12.3",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.3"
  }
}
